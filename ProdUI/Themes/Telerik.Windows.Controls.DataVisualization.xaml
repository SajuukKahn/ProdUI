    <Style x:Key="TimelineItemGroupControlToggleButtonStyle" TargetType="telerik1:RadToggleButton">
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="BorderBrush" Value="Transparent" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="CornerRadius" Value="0" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik1:RadToggleButton">
                    <Grid Background="#01FFFFFF">
                        <StackPanel Orientation="Horizontal">
                            <Border x:Name="buttonBorder"
                                    Margin="3,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    Background="{TemplateBinding Background}"
                                    BorderBrush="{TemplateBinding BorderBrush}"
                                    BorderThickness="{TemplateBinding BorderThickness}"
                                    CornerRadius="{TemplateBinding CornerRadius}">
                                <Grid>
                                    <TextBlock x:Name="expandButton"
                                               HorizontalAlignment="Center"
                                               VerticalAlignment="Center"
                                               telerik:GlyphAdorner.ShowGlyphInDesignTime="True"
                                               FontFamily="{StaticResource TelerikWebUI}"
                                               FontSize="16"
                                               FontStyle="Normal"
                                               FontWeight="Normal"
                                               Foreground="{telerik1:MaterialResource ResourceKey=IconBrush}"
                                               Opacity="{telerik1:MaterialResource ResourceKey=SecondaryOpacity}"
                                               Visibility="Visible">
                                        <Run Text="{StaticResource GlyphPlus}" />
                                    </TextBlock>
                                    <TextBlock x:Name="collapseButton"
                                               HorizontalAlignment="Center"
                                               VerticalAlignment="Center"
                                               telerik:GlyphAdorner.ShowGlyphInDesignTime="True"
                                               FontFamily="{StaticResource TelerikWebUI}"
                                               FontSize="16"
                                               FontStyle="Normal"
                                               FontWeight="Normal"
                                               Foreground="{telerik1:MaterialResource ResourceKey=IconBrush}"
                                               Opacity="{telerik1:MaterialResource ResourceKey=SecondaryOpacity}"
                                               Visibility="Collapsed">
                                        <Run Text="{StaticResource GlyphMinus}" />
                                    </TextBlock>
                                </Grid>
                            </Border>
                            <TextBlock x:Name="groupContent"
                                       Margin="4,0,0,0"
                                       Opacity="{telerik1:MaterialResource ResourceKey=SecondaryOpacity}"
                                       Text="{TemplateBinding Content}" />
                        </StackPanel>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="expandButton" Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=PrimaryHoverBrush}" />
                            <Setter TargetName="collapseButton" Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=PrimaryHoverBrush}" />
                            <Setter TargetName="expandButton" Property="Opacity" Value="1" />
                            <Setter TargetName="collapseButton" Property="Opacity" Value="1" />
                        </Trigger>
                        <Trigger Property="IsChecked" Value="True">
                            <Setter TargetName="collapseButton" Property="Visibility" Value="Visible" />
                            <Setter TargetName="expandButton" Property="Visibility" Value="Collapsed" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="expandButton" Property="Opacity" Value="{telerik1:MaterialResource ResourceKey=DisabledOpacity}" />
                            <Setter TargetName="collapseButton" Property="Opacity" Value="{telerik1:MaterialResource ResourceKey=DisabledOpacity}" />
                            <Setter TargetName="groupContent" Property="Opacity" Value="{telerik1:MaterialResource ResourceKey=DisabledOpacity}" />
                        </Trigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsEnabled" Value="False" />
                                <Condition Property="IsChecked" Value="True" />
                            </MultiTrigger.Conditions>
                            <Setter TargetName="expandButton" Property="Opacity" Value="{telerik1:MaterialResource ResourceKey=DisabledOpacity}" />
                            <Setter TargetName="collapseButton" Property="Opacity" Value="{telerik1:MaterialResource ResourceKey=DisabledOpacity}" />
                            <Setter TargetName="groupContent" Property="Opacity" Value="{telerik1:MaterialResource ResourceKey=DisabledOpacity}" />
                        </MultiTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
