    <Style x:Key="RadDataBarStyle" TargetType="datavizControls:RadDataBar">
        <Setter Property="FontFamily" Value="{telerik1:MaterialResource ResourceKey=FontFamily}" />
        <Setter Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=MarkerBrush}" />
        <Setter Property="ValueBrush" Value="{StaticResource MaterialThemeBrush1}" />
        <Setter Property="ValueBorderBrush" Value="{StaticResource MaterialThemeBrush1}" />
        <Setter Property="NegativeValueBrush" Value="{telerik1:MaterialResource ResourceKey=ValidationBrush}" />
        <Setter Property="NegativeValueBorderBrush" Value="{telerik1:MaterialResource ResourceKey=ValidationBrush}" />
        <Setter Property="AxisVisibility" Value="Collapsed" />
        <Setter Property="AxisStroke" Value="{telerik1:MaterialResource ResourceKey=DividerBrush}" />
        <Setter Property="AxisStyle" Value="{StaticResource DataBarAxisStyle}" />
        <Setter Property="BarHeightPercent" Value="0.66" />
        <Setter Property="BarBorderThickness" Value="{StaticResource DataBarsBorderThickness}" />
        <Setter Property="BarStyle" Value="{StaticResource DataBarShapeStyle}" />
        <Setter Property="LabelDistance" Value="5" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="datavizControls:RadDataBar">
                    <Border Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}">
                        <Grid>
                            <dataBars:DataBarShape Fill="{TemplateBinding AppliedBarBrush}"
                                                   HeightPercent="{TemplateBinding BarHeightPercent}"
                                                   LeftPercent="{TemplateBinding BarLeftPercent}"
                                                   OutOfRangeRule="{TemplateBinding AppliedOutOfRangeRule}"
                                                   RightPercent="{TemplateBinding BarRightPercent}"
                                                   Stroke="{TemplateBinding AppliedBarBorderBrush}"
                                                   StrokeThickness="{TemplateBinding BarBorderThickness}"
                                                   Style="{TemplateBinding BarStyle}" />
                            <ContentPresenter ContentTemplate="{TemplateBinding AppliedOutOfRangeTemplate}" />
                            <Rectangle Margin="{TemplateBinding AppliedAxisMargin}"
                                       HorizontalAlignment="Left"
                                       Fill="{TemplateBinding AxisStroke}"
                                       Style="{TemplateBinding AxisStyle}"
                                       Visibility="{TemplateBinding AppliedAxisVisibility}" />
                            <TextBlock Margin="{TemplateBinding AppliedLabelMargin}"
                                       VerticalAlignment="Center"
                                       Style="{TemplateBinding LabelStyle}"
                                       Text="{TemplateBinding LabelText}"
                                       Visibility="{TemplateBinding LabelVisibility}" />
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
