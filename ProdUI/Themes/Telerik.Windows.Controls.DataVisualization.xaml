                                       FontSize="16"
                                       FontStyle="Normal"
                                       FontWeight="Normal"
                                       Foreground="{telerik1:MaterialResource ResourceKey=MarkerInvertedBrush}"
                                       SnapsToDevicePixels="True">
                                <Run Text="{StaticResource GlyphZoom}" />
                            </TextBlock>
                        </telerik:RadDropDownButton>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
