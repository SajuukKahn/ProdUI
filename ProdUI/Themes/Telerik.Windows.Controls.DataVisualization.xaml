    <Style x:Key="VisualizationLayerStyle" TargetType="maps:VisualizationLayer">
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="DefaultClusterTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Grid x:Name="ParentRoot"
                          Width="40"
                          Height="25"
                          maps:MapLayer.HotSpot="0.5,1">
                        <Path Width="{Binding Width, ElementName=ParentRoot}"
                              Height="{Binding Height, ElementName=ParentRoot}"
                              Data="{StaticResource ClusterIcon}"
                              Fill="#7F000000"
                              Stretch="Fill"
                              Stroke="White"
                              StrokeEndLineCap="Flat"
                              StrokeLineJoin="Miter"
                              StrokeMiterLimit="10"
                              StrokeStartLineCap="Flat"
                              StrokeThickness="1" />
                        <StackPanel Margin="5,2,5,5">
                            <TextBlock HorizontalAlignment="Center"
                                       Foreground="White"
                                       Text="{Binding Path=Count}" />
                        </StackPanel>
                    </Grid>
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="DefaultItemTemplate">
            <Setter.Value>
                <DataTemplate>
                    <maps:Pushpin />
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="DefaultKmlItemTemplate">
            <Setter.Value>
                <DataTemplate>
                    <StackPanel x:Name="PART_Panel"
                                maps:MapLayer.HotSpot="{Binding HotSpot}"
                                maps:MapLayer.Location="{Binding Location}"
                                Orientation="Horizontal"
                                ToolTipService.ToolTip="{Binding ToolTip}">
                        <Image x:Name="PART_Image"
                               RenderTransform="{Binding ImageTransform}"
                               Source="{Binding ImageSource}"
                               Stretch="None"
                               Visibility="{Binding ImageVisibility}" />
                        <TextBlock x:Name="PART_Text"
                                   Margin="2,0,0,0"
                                   VerticalAlignment="Center"
                                   Text="{Binding Text}" />
                    </StackPanel>
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="ShapeTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Path x:Name="PART_RangePath"
                          Data="{Binding Path=Geometry, Mode=OneWay}"
                          Fill="{Binding ActiveShapeFill.Fill}"
                          RenderTransform="{Binding Path=Transform, Mode=OneWay}"
                          RenderTransformOrigin="{Binding Path=RenderTransformOrigin, Mode=OneWay}"
                          Stroke="{Binding ActiveShapeFill.Stroke}"
                          StrokeDashArray="{Binding ActiveShapeFill.StrokeDashArray}"
                          StrokeDashCap="{Binding ActiveShapeFill.StrokeDashCap}"
                          StrokeDashOffset="{Binding ActiveShapeFill.StrokeDashOffset}"
                          StrokeEndLineCap="{Binding ActiveShapeFill.StrokeEndLineCap}"
                          StrokeLineJoin="{Binding ActiveShapeFill.StrokeLineJoin}"
                          StrokeMiterLimit="{Binding ActiveShapeFill.StrokeMiterLimit}"
                          StrokeStartLineCap="{Binding ActiveShapeFill.StrokeStartLineCap}"
                          StrokeThickness="{Binding ActiveShapeFill.StrokeThickness}" />
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
