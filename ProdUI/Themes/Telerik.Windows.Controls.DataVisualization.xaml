    <Style x:Key="RadTimeBarStyle" TargetType="datavizControls:RadTimeBar">
        <Setter Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=MarkerBrush}" />
        <Setter Property="Background" Value="{telerik1:MaterialResource ResourceKey=MainBrush}" />
        <Setter Property="BorderBrush" Value="{telerik1:MaterialResource ResourceKey=DividerBrush}" />
        <Setter Property="FontFamily" Value="{telerik1:MaterialResource ResourceKey=FontFamily}" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="Padding" Value="0,0,0,2" />
        <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth1" />
        <Setter Property="SliderStyle" Value="{StaticResource TimeBar_SliderStyle}" />
        <Setter Property="SelectionThumbStyle" Value="{StaticResource SelectionThumbStyle}" />
        <Setter Property="SelectionVisibilityIndicatorStyle" Value="{StaticResource SelectionVisibilityIndicatorStyle}" />
        <Setter Property="PeriodHeaderStyle" Value="{StaticResource TimeBar_PeriodHeaderStyle}" />
        <Setter Property="GroupPeriodHeaderStyle" Value="{StaticResource TimeBar_GroupPeriodHeaderStyle}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="datavizControls:RadTimeBar">
                    <Border Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            SnapsToDevicePixels="True">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <mat:Shadow Grid.RowSpan="2"
                                        Background="{TemplateBinding Background}"
                                        ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}" />
                            <Grid Grid.Row="0">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="40" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                <Border Grid.Row="0"
                                        Height="4"
                                        VerticalAlignment="Bottom"
                                        Background="{telerik1:MaterialResource ResourceKey=DividerBrush}" />
                                <Border Grid.Row="1"
                                        Grid.RowSpan="2"
                                        Background="{TemplateBinding Background}" />
                                <timeBars:TimeSpecialSlotContainer x:Name="PART_TimeSpecialSlotContainer"
                                                                   Grid.Row="1"
                                                                   Grid.RowSpan="2"
                                                                   ItemsSource="{Binding Path=SpecialSlots, RelativeSource={RelativeSource TemplatedParent}}">
                                    <timeBars:TimeSpecialSlotContainer.ItemsPanel>
                                        <ItemsPanelTemplate>
                                            <timeBars:TimeBarPanel Slider="{Binding ElementName=PART_Slider}" />
                                        </ItemsPanelTemplate>
                                    </timeBars:TimeSpecialSlotContainer.ItemsPanel>
                                    <timeBars:TimeSpecialSlotContainer.Template>
                                        <ControlTemplate>
                                            <ItemsPresenter />
                                        </ControlTemplate>
                                    </timeBars:TimeSpecialSlotContainer.Template>
                                </timeBars:TimeSpecialSlotContainer>
                                <timeBars:TimeBarContentPanel Grid.Row="2"
                                                              Margin="0,2,0,0"
                                                              Slider="{Binding ElementName=PART_Slider}"
                                                              SliderMaximum="{Binding ElementName=PART_Slider, Path=Maximum, Mode=TwoWay}"
                                                              SliderMinimum="{Binding ElementName=PART_Slider, Path=Minimum, Mode=TwoWay}">
                                    <ContentPresenter />
                                </timeBars:TimeBarContentPanel>
                                <timeBars:GroupContainer x:Name="PART_GroupContainer"
                                                         Grid.Row="1"
                                                         ItemHeaderStyle="{TemplateBinding GroupPeriodHeaderStyle}"
                                                         ItemsSource="{TemplateBinding IntervalGroups}"
                                                         Selection="{Binding Path=Selection, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}">
                                    <timeBars:GroupContainer.ItemsPanel>
                                        <ItemsPanelTemplate>
                                            <timeBars:LabelMeasurementPanel Slider="{Binding ElementName=PART_Slider}" />
                                        </ItemsPanelTemplate>
                                    </timeBars:GroupContainer.ItemsPanel>
                                    <timeBars:GroupContainer.Template>
                                        <ControlTemplate>
                                            <ItemsPresenter />
                                        </ControlTemplate>
                                    </timeBars:GroupContainer.Template>
                                </timeBars:GroupContainer>
                                <timeBars:ItemContainer x:Name="PART_ItemContainer"
                                                        Grid.Row="0"
                                                        Grid.RowSpan="3"
                                                        ItemHeaderStyle="{TemplateBinding PeriodHeaderStyle}"
                                                        ItemsSource="{TemplateBinding IntervalItems}"
                                                        Selection="{Binding Path=Selection, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}">
                                    <timeBars:ItemContainer.ItemsPanel>
                                        <ItemsPanelTemplate>
                                            <timeBars:LabelMeasurementPanel Slider="{Binding ElementName=PART_Slider}" />
                                        </ItemsPanelTemplate>
                                    </timeBars:ItemContainer.ItemsPanel>
                                    <timeBars:ItemContainer.Template>
                                        <ControlTemplate>
                                            <ItemsPresenter />
                                        </ControlTemplate>
                                    </timeBars:ItemContainer.Template>
                                </timeBars:ItemContainer>
                                <timeBars:TimeBarStackPanel Grid.Row="1"
                                                            Grid.RowSpan="2"
                                                            Slider="{Binding ElementName=PART_Slider}"
                                                            SliderMaximum="{Binding ElementName=PART_Slider, Path=Maximum, Mode=TwoWay}"
                                                            SliderMinimum="{Binding ElementName=PART_Slider, Path=Minimum, Mode=TwoWay}">
                                    <timeBars:ShadeControl EndDate="{Binding ElementName=PART_SelectionThumb, Path=StartDate}" StartDate="{TemplateBinding PeriodStart}" />
                                    <timeBars:SelectionThumb x:Name="PART_SelectionThumb"
                                                             Canvas.ZIndex="2"
                                                             EndDate="{Binding Path=ActualSelectionEnd, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"
                                                             IntervalGroups="{TemplateBinding IntervalGroups}"
                                                             IntervalItems="{TemplateBinding IntervalItems}"
                                                             IsSnapToIntervalEnabled="{TemplateBinding IsSnapToIntervalEnabled}"
                                                             MaxSelectionRange="{TemplateBinding MaxSelectionRange}"
                                                             MinSelectionRange="{TemplateBinding MinSelectionRange}"
                                                             PeriodEnd="{TemplateBinding PeriodEnd}"
                                                             PeriodStart="{TemplateBinding PeriodStart}"
                                                             Selection="{Binding Path=Selection, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"
                                                             SelectionPreviewControl="{Binding ElementName=PART_SelectionThumbPreview}"
                                                             StartDate="{Binding Path=ActualSelectionStart, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"
                                                             Style="{TemplateBinding SelectionThumbStyle}"
                                                             TitleFormatString="{TemplateBinding SelectionTitleFormatString}"
                                                             VisiblePeriodRange="{Binding Path=ActualVisiblePeriodRange, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}" />
                                    <timeBars:ShadeControl EndDate="{TemplateBinding PeriodEnd}" StartDate="{Binding ElementName=PART_SelectionThumb, Path=EndDate}" />
                                </timeBars:TimeBarStackPanel>
                                <timeBars:TimeBarPanel Grid.Row="1"
                                                       Grid.RowSpan="2"
                                                       Slider="{Binding ElementName=PART_Slider}"
                                                       SliderMaximum="{Binding ElementName=PART_Slider, Path=Maximum, Mode=TwoWay}"
                                                       SliderMinimum="{Binding ElementName=PART_Slider, Path=Minimum, Mode=TwoWay}">
                                    <timeBars:SelectionThumbPreview x:Name="PART_SelectionThumbPreview" />
                                </timeBars:TimeBarPanel>
                            </Grid>
                            <timeBars:SelectionVisibilityIndicator Grid.Row="0"
                                                                   HorizontalAlignment="Left"
                                                                   RenderTransformOrigin="0.5 0.5"
                                                                   Style="{TemplateBinding SelectionVisibilityIndicatorStyle}"
                                                                   Visibility="{TemplateBinding LeftSelectionIndicatorVisibility}"
                                                                   VisibilityListener="{TemplateBinding LeftSelectionIndicatorVisibility}">
                                <timeBars:SelectionVisibilityIndicator.RenderTransform>
                                    <RotateTransform Angle="180" />
                                </timeBars:SelectionVisibilityIndicator.RenderTransform>
                            </timeBars:SelectionVisibilityIndicator>
                            <timeBars:SelectionVisibilityIndicator Grid.Row="0"
                                                                   HorizontalAlignment="Right"
                                                                   Style="{TemplateBinding SelectionVisibilityIndicatorStyle}"
                                                                   Visibility="{TemplateBinding RightSelectionIndicatorVisibility}"
                                                                   VisibilityListener="{TemplateBinding RightSelectionIndicatorVisibility}" />
                            <telerik:RadSlider x:Name="PART_Slider"
                                               Grid.Row="1"
                                               IsCoercionSuppressed="True"
                                               LargeChange="{Binding Path=LargeChange, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource timespanConverter}}"
                                               Maximum="{Binding Path=PeriodEnd, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource dateConverter}}"
                                               MaximumRangeSpan="{Binding Path=MaxZoomRange, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource timeSpanToMaximumRangeSpanConverter}}"
                                               Minimum="{Binding Path=PeriodStart, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource dateConverter}}"
                                               MinimumRangeSpan="{Binding Path=ActualMinZoomRange, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource timespanConverter}}"
                                               Selection="{Binding Path=ActualVisiblePeriodRange, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, Converter={StaticResource selectionRangeConverter}}"
                                               SmallChange="{Binding Path=SmallChange, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource timespanConverter}}"
                                               Style="{TemplateBinding SliderStyle}"
                                               ThumbVisibility="{Binding Path=ScrollMode, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource scrollModeToThumbVisibilityConverter}}"
                                               Visibility="{Binding Path=ScrollMode, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource scrollModeToVisibilityConverter}}" />
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
