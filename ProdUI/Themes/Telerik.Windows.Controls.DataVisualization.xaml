                          Height="{TemplateBinding CellHeight}"
                          Background="{TemplateBinding Background}">
                        <Grid HorizontalAlignment="Stretch"
                              VerticalAlignment="Stretch"
                              RenderTransformOrigin="0.5,0.5">
                            <Grid.RenderTransform>
                                <TransformGroup>
                                    <ScaleTransform ScaleX="-1" />
                                </TransformGroup>
                            </Grid.RenderTransform>
                            <Path Data="{StaticResource SemicircleEastAndWestGaugeData}"
                                  Fill="{TemplateBinding OuterBackground}"
                                  Stretch="Fill"
                                  Stroke="{TemplateBinding OuterBorderBrush}"
                                  StrokeThickness="{TemplateBinding OuterBorderThickness}" />
