                            <telerik:RadDropDownButton.DropDownContent>
                                <Grid x:Name="PART_DropDownPopupWrapper"
                                      Margin="5,5,5,10"
                                      HorizontalAlignment="Stretch"
                                      VerticalAlignment="Stretch"
                                      Background="#01ffffff">
                                    <Border Width="36"
                                            HorizontalAlignment="Left"
                                            Background="{telerik1:MaterialResource ResourceKey=MainBrush}"
                                            BorderBrush="{telerik1:MaterialResource ResourceKey=DividerBrush}"
