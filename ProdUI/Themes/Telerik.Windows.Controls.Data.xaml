    <ControlTemplate x:Key="ToggleButtonTemplate" TargetType="telerik:RadToggleButton">
        <Border Background="Transparent" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
            <Border
                    HorizontalAlignment="Center"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    VerticalAlignment="Center"
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
                <Grid>
                    <TextBlock x:Name="Plus"
                            telerik:GlyphAdorner.ShowGlyphInDesignTime="True"
                            Opacity="{telerik1:MaterialResource ResourceKey=SecondaryOpacity}"
                            FontFamily="{StaticResource TelerikWebUI}"
                            Foreground="{TemplateBinding Foreground}"
                            FontSize="16"
                            FontStyle="Normal"
                            FontWeight="Normal"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center">
                        <Run Text="{StaticResource GlyphPlus}"/>
                    </TextBlock>
                    <TextBlock x:Name="Minus"
                            telerik:GlyphAdorner.ShowGlyphInDesignTime="True"
                            FontFamily="{StaticResource TelerikWebUI}"
                            Foreground="{TemplateBinding Foreground}"
                            FontSize="16"
                            FontStyle="Normal"
                            FontWeight="Normal"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            Visibility="Collapsed"
                            Opacity="{telerik1:MaterialResource ResourceKey=SecondaryOpacity}">
                        <Run Text="{StaticResource GlyphMinus}"/>
                    </TextBlock>
                </Grid>
            </Border>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="Plus" Property="Opacity" Value="1"/>
                <Setter TargetName="Minus" Property="Opacity" Value="1"/>
                <Setter Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=PrimaryHoverBrush}"/>
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter TargetName="Plus" Property="Opacity" Value="1"/>
                <Setter TargetName="Minus" Property="Opacity" Value="1"/>
                <Setter Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=PrimaryPressedBrush}"/>
            </Trigger>
            <Trigger Property="IsChecked" Value="True">
                <Setter TargetName="Plus" Property="Visibility" Value="Collapsed"/>
                <Setter TargetName="Minus" Property="Visibility" Value="Visible"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
